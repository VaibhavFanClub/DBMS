----------------------------------------------- set c 1 -------------------------------------------

assignment1=# create table cities(city char(20) primary key, state char(20));
CREATE TABLE
assignment1=# create table warehouse(wid int primary key, wname char(30), location char(20), city char(20) references cities);
CREATE TABLE
assignment1=# create table store(sid int primary key, sname char(20), location_city char(20), wid int references warehouse);
CREATE TABLE
assignment1=# create table cust(cno int primary key, cname char(50), addr varchar(50), cu_city char(20));
CREATE TABLE
assignment1=# create table orders(ono int primary key, odate date, cno int references cust);
CREATE TABLE
assignment1=# create table items(ino int primary key, description text, weight decimal(5,2), cost decimal(5,2));
CREATE TABLE
assignment1=# create table i_o(ino int references items, ono int references orders);
CREATE TABLE
assignment1=# create table s_i(sid int references store, ino int references items);
